{
	"name": "@iconify/tools",
	"description": "Collection of functions for cleaning up and parsing SVG for Iconify project",
	"author": "Vjacheslav Trushkin",
	"license": "MIT",
	"bugs": "https://github.com/iconify/tools/issues",
	"homepage": "https://github.com/iconify/tools",
	"repository": {
		"type": "git",
		"url": "git://github.com/iconify/tools.git"
	},
	"workspaces": [
		"@iconify/*",
		"@debug/*",
		"@iconify-demo/*"
	],
	"scripts": {
		"build": "npm run build --workspaces --if-present",
		"build:tools": "npm run build --workspace=@iconify/tools",
		"test": "npm run test --workspaces --if-present",
		"test:tools": "npm run test --workspace=@iconify/tools"
	},
	"devDependencies": {
		"@iconify/iconify": "^2.1.0",
		"@iconify/json": "^2.0.1",
		"@iconify/library-builder": "^1.0.4",
		"@mdi/svg": "^6.5.95",
		"@types/jest": "^27.0.1",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"eslint-plugin-jasmine": "^4.1.2",
		"jasmine": "^3.9.0",
		"jest": "^27.2.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.0.5",
		"tsup": "^5.5.0",
		"typescript": "^4.4.3"
	}
}
